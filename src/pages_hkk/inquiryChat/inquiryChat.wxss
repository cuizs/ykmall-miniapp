page {
  background-color: #f9f9f9;
}
.inquiry-status-bar-gray,
.inquiry-status-bar {
  height: 70rpx;
  width: 100%;
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 200;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 10rpx 30rpx 0 60rpx;
}
.inquiry-status-bar-gray::before,
.inquiry-status-bar::before {
  display: block;
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  height: 10rpx;
  width: 100%;
  background-color: #f9f9f9;
}
.inquiry-status-bar-gray::after,
.inquiry-status-bar::after {
  content: '';
  position: absolute;
  top: calc(50% + 5rpx);
  transform: translateY(-50%);
  left: 30rpx;
  width: 8rpx;
  height: 38rpx;
  background: rgba(120, 207, 82, 1);
}
.inquiry-status-bar-gray::after {
  background: #969696;
}

.inquiry-status-bar-text {
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
}
.inquiry-status-bar-link {
  font-size: 28rpx;
  font-weight: 400;
  color: #1b90e6;
}
.scrollView {
  /* padding-top: 60rpx; */
}
.msgItemLeft {
  padding: 2vw 11vw 2vw 2vw;
  display: flex;
  justify-content: flex-start;
}
.msgItemRight {
  padding: 2vw 2vw 2vw 11vw;
  display: flex;
  justify-content: flex-end;
}
/* 头像相关 */
.userAvatar {
  width: 11vw;
  height: 11vw;
}
.userAvatarRight {
  margin-left: 5vw;
}
.userAvatarLeft {
  margin-right: 5vw;
}
.userAvatar image {
  width: 11vw;
  height: 11vw;
  border-radius: 50%;
}

/* 文字消息 */
.msgText {
  font-size: 32rpx;
  color: #444;
  line-height: 7vw;
  padding: 2vw 2.5vw;
  background-color: #fff;
  margin-left: -1.6vw;
  border-radius: 10rpx;
  z-index: 10;
}
.serverMsgText {
  background-color: #f7f4ff;
}
.blanck-top {
  height: 70rpx;
  width: 100%;
}
/* 占位底部菜单 */
.blanck {
  width: 100%;
  /* 必须大于inputRoom高度 */
  /* height: 20vw; */
  height: calc(110rpx + constant(safe-area-inset-bottom));
  height: calc(110rpx + env(safe-area-inset-bottom));
}

/* 底部菜单栏 */
.bottomInputRoomIOS {
  width: 100vw;
  background-color: #eeeeee;
  position: fixed;
  bottom: 0;
  display: flex;
  align-items: bottom;
  z-index: 20;
  padding: 20rpx 0;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  box-sizing: border-box;
}
.bottomInputRoomAndroid {
  width: 100vw;
  background-color: #eeeeee;
  position: fixed;
  bottom: 0;
  display: flex;
  align-items: bottom;
  z-index: 20;
